#include <bits/stdc++.h>
using namespace std;

int main() {
	int a[5][5];
	int indexi=0,indexj=0;
	for(int i=0;i<5;i++)
	{
	    for(int j=0;j<5;j++)
	    {
	        cin>>a[i][j];
	        if(a[i][j] == 1)
	        {
	            indexi= i;
	            indexj= j;
	        }
	    }
	}
	int ans = abs(3-indexi) + abs(3-indexj);
	cout<<ans<<endl;;
	return 0;
}
